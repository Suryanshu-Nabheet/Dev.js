{
  "name": "flight-parcel",
  "private": true,
  "source": "src/server.tsx",
  "server": "dist/server.js",
  "targets": {
    "server": {
      "context": "devjs-server",
      "includeNodeModules": {
        "express": false
      }
    }
  },
  "scripts": {
    "predev": "cp -r ../../build/oss-experimental/* ./node_modules/",
    "prebuild": "cp -r ../../build/oss-experimental/* ./node_modules/",
    "dev": "parcel",
    "build": "parcel build",
    "start": "node dist/server.js"
  },
  "dependencies": {
    "@types/parcel-env": "^0.0.6",
    "@types/express": "*",
    "@types/node": "^22.10.1",
    "@types/devjs": "^19",
    "@types/devjs-dom": "^19",
    "concurrently": "^7.3.0",
    "express": "^4.18.2",
    "parcel": "canary",
    "process": "^0.11.10",
    "devjs": "experimental",
    "devjs-dom": "experimental",
    "devjs-server-dom-parcel": "experimental",
    "rsc-html-stream": "^0.0.4"
  }
}
